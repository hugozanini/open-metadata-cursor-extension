{
  "name": "openmetadata-ai-explorer",
  "displayName": "OpenMetadata AI Explorer",
  "description": "AI-powered data discovery and lineage visualization for OpenMetadata",
  "version": "1.1.0",
  "publisher": "openmetadata",
  "author": {
    "name": "Hugo Zanini",
    "email": "your.email@company.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hugozanini/open-metadata-cursor-extension.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "openmetadata",
    "data-catalog",
    "lineage",
    "ai",
    "data-discovery",
    "voice-coding",
    "dictation"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "openmetadataPanel",
          "title": "OPEN METADATA",
          "icon": "$(database)"
        }
      ]
    },
    "views": {
      "openmetadataPanel": [
        {
          "id": "openmetadataExplorer",
          "name": "Explorer",
          "type": "webview"
        }
      ]
    },
    "configuration": {
      "title": "OpenMetadata",
      "properties": {
        "openmetadataExplorer.geminiApiKey": {
          "type": "string",
          "description": "Your Gemini API key from AI Studio",
          "default": ""
        },
        "openmetadataExplorer.openmetadataUrl": {
          "type": "string",
          "default": "http://localhost:8585",
          "description": "OpenMetadata server URL"
        },
        "openmetadataExplorer.openmetadataAuthToken": {
          "type": "string",
          "description": "OpenMetadata authentication token (leave empty if auth is disabled)",
          "default": ""
        },
        "openmetadataExplorer.deepgramApiKey": {
          "type": "string",
          "description": "Deepgram API key for voice features",
          "default": ""
        },
        "openmetadataExplorer.microphone.deviceMacOS": {
          "type": "string",
          "description": "Microphone device to use on macOS (leave empty for default)",
          "default": ""
        },
        "openmetadataExplorer.microphone.deviceWindows": {
          "type": "string",
          "description": "Microphone device to use on Windows (leave empty for default)",
          "default": ""
        },
        "openmetadataExplorer.microphone.deviceLinux": {
          "type": "string",
          "description": "Microphone device to use on Linux (leave empty for default)",
          "default": ""
        }
      }
    },
    "commands": [
      {
        "command": "openmetadataExplorer.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "openmetadataExplorer.openVibeCoder",
        "title": "Open Voice Coding Assistant"
      },
      {
        "command": "openmetadataExplorer.toggleDictation",
        "title": "Toggle Dictation"
      }
    ],
    "keybindings": [
      {
        "command": "openmetadataExplorer.toggleDictation",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "!inDebugRepl"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "package": "npm run compile && npx @vscode/vsce package",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.28.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.0.0",
    "@huggingface/transformers": "^3.7.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "elkjs": "^0.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4",
    "speex-resampler": "^3.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  }
}
